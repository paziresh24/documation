    "title": "Booking APIs",
    "version": "v1.0.0",
    "description": "APIs related to booking appointments and retrieving free time slots."
  },
  "servers": [
    {
      "url": "https://apigw.paziresh24.com"
    }
  ],
  "security": [
    {
      "BearerAuth": []
